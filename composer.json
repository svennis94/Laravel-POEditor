{
    "name": "sebudesign/laravel-poeditor",
    "description": "A Laravel package for POEditor",
    "keywords": [
        "sebudesign",
        "laravel",
        "poeditor",
        "i18n",
        "translations"
    ],
    "homepage": "https://github.com/SeBuDesign/Laravel-POEditor",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Buijsrogge",
            "email": "sven@sebudesign.nl",
            "homepage": "https://sebu.design",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/container": "^5.3.0",
        "illuminate/contracts": "^5.3.0",
        "illuminate/database": "^5.4.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.3.0|~3.4.2|^3.5.0",
        "phpunit/phpunit": "^5.7|6.2|^7.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "SeBuDesign\\PoEditor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SeBuDesign\\PoEditor\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SeBuDesign\\PoEditor\\PoEditorServiceProvider"
            ]
        }
    }
}
